<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions" meta_type="CMF Action Category">
  <object name="twitter" meta_type="CMF Action" i18n:domain="ccc.theme">
   <property name="title" i18n:translate="">Twitter</property>
   <property name="description" i18n:translate="">Follow us on Twitter</property>
   <property name="url_expr">string:https://twitter.com/cleanclothes</property>
   <property name="link_target"></property>
   <property name="icon_expr">string:$portal_url/++theme++ccc/icons/twitter.png</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
